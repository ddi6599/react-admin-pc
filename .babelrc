{
    "presets": [
        "react",
        "es2015",
        
        ["env", {
            "modules": false,
            "targets": {
                "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
            }
        }],
        "stage-2"
    ],
    "plugins": [
        ["transform-runtime", { "polyfill": true }],
        /*if style: css will ERROR in ./~/antd/lib/spin/style/index.css*/
        ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": "true" }], // `style: true` 会加载 less 文件
        "transform-decorators-legacy"
    ],
    "env": {
        "test": {
            "presets": ["env", "stage-2"]
        }
    }
}